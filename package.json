{
  "name": "blog",
  "version": "1.0.0",
  "description": "个人博客",
  "main": "index.js",
  "author": "华总",
  "license": "MIT",
  "scripts": {
    "docs:dev": "set NODE_OPTIONS=--openssl-legacy-provider && vuepress dev docs",
    "docs:build": "set NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=102400 ./node_modules/vuepress/cli.js build docs",
    "docs:nav": "vuepress nav docs -f",
    "deploy": "./docs/.vuepress/deploy.sh",
    "deploy:eop": "yarn run docs:build && edgeone pages deploy ./docs/.vuepress/dist -n vuepress-vdoing",
    "build": "./docs/.vuepress/build.sh",
    "push": "./docs/push.sh",
    "editFm": "node ./frontmatter/editFrontmatter.mjs",
    "sitemap": "./docs/generate_sitemap.sh",
    "postinstall": "patch-package",
    "clean-urls": "node ./utils/cleanImageUrls.js",
    "clean-urls:apply": "node ./utils/cleanImageUrls.js --apply"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "core-js": "2.6.12",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.4",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.10",
    "json2yaml": "^1.1.0",
    "moment": "^2.29.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "vue-tabs-component": "^1.5.0",
    "vuepress": "^1.9.10",
    "vuepress-plugin-img-lazy": "^1.0.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@docsearch/css": "^4.5.3",
    "@docsearch/js": "^4.5.3",
    "@vuepress/plugin-active-header-links": "^1.9.10",
    "@vuepress/plugin-back-to-top": "^1.9.10",
    "dotenv": "^17.2.3",
    "sass": "^1.97.3",
    "sass-loader": "^13.3.2",
    "v-dialogs": "^2.2.0",
    "vuepress-plugin-blocktoggle": "^1.1.5",
    "vuepress-plugin-container": "^2.1.5",
    "vuepress-plugin-copycodeblock": "^1.0.1",
    "vuepress-plugin-element-ui": "^1.1.0",
    "vuepress-plugin-twikoo": "^1.0.20",
    "vuepress-theme-vdoing": "^1.12.9"
  }
}
